{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": ["components/*"],
      "layouts/*": ["layouts/*"],
      "utils": ["utils"],
      "store/*": ["store/*"],
      "hooks": ["hooks"],
      "config": ["config"],
      "assets/*": ["assets/*"],
      
    },
    "downlevelIteration": true
  }
}
